<tr id=<%= anchor ||= '' %>>
  <td align="center" class="mp0x5" style="padding: 0 30px;">
    <table align="left" border="0" cellpadding="0" cellspacing="0" width="100%">
      <tbody>
        <tr>
          <td align="center" class="mobile-pb30" style="padding-left: 11px; padding-right: 11px; padding-bottom: 40px;">
            <table border="0" cellpadding="0" cellspacing="0" class="board chameleon-line" height="100%" style="border-radius: 8px; background-color: #fff" width="100%">
              <tbody class="board__body">
                <tr class="board__row">
                  <td class="board__cell" style="min-width: 174px; text-align: center;" valign="top" width="174">

                    <!-- left inner -->
                    <table border="0" cellpadding="0" cellspacing="0" height="100%" width="100%">
                      <tbody>
                        <tr>
                          <td align="center" style="padding: 20px 18px 19px;" valign="top">

                            <!-- stat -->
                            <table border="0" cellpadding="0" cellspacing="0" style="margin:0 auto;">
                              <tbody>
                                <tr>
                                  <td class="stat-head" colspan="2" style="padding-bottom: 8px; text-align: center;">
                                    <b class="stat-heading" style="font-family:Arial, Helvetica, sans-serif; font-size:13px; line-height:22px; font-weight:700;color:#A0A0A0;"> 
                                      <%= title %>
                                    </b>
                                  </td>
                                </tr>
                                <tr style="text-align: center">
                                  <td valign="top" width="38" colspan="2">
                                    <b class="stat-value chameleon-heading" style="font-family:Arial, Helvetica, sans-serif; font-size:28px; line-height:32px; font-weight:700;color:#4b4b4b;">
                                      <%= list[:data].length %>
                                    </b>
                                  </td>
                                </tr>
                              </tbody>
                            </table> <!--/stat -->
                            <div style="border-radius:8px; margin: -57px 1px; color: #167ef44f; font-weight: 400; background: #dcf6dc; padding: 4px 6px; float:right;">
                              <%= number_to_currency(list[:totalValue] / 100) %>
                            </div>
                          </td>
                        </tr>
                      </tbody>
                    </table> <!--/left inner -->

                  </td>
                </tr>
                <tr class="board__row">
                  <td class="board__cell chameleon-line" style="border-top: 2px solid #E5E5E5;" valign="top" width="340">

                    <!-- right inner -->
                    <table border="0" cellpadding="0" cellspacing="0" width="100%">
                      <tbody>
                        <tr>
                          <td align="" class="days-chart-column" style="padding: 20px 16px 22px 17px;" valign="top">
                            <table border="0" cellpadding="0" cellspacing="0" style="" width="100%">
                              <tbody>
                                <tr>
                                  <td>
                                    <table width="100%">
                                      <%= render partial: 'customer_list', collection: list[:data], as: :item %>
                                    </table>
                                  </td>
                                </tr>
                              </tbody>
                            </table>
                          </td>
                        </tr>
                      </tbody>
                    </table> <!--/right inner -->
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
      </tbody>
    </table>
  </td>
</tr>
