
<table border="0" cellpadding="0" cellspacing="0" width="100%">
  <tbody>
    <tr style="">
      <td align="" class="card-head" colspan="4" style="padding: 4px;">
        <span class="card-heading" style="font-family:Helvetica, sans-serif; font-size:16px; line-height:22px; font-weight:100;color:#4b4b4b;">
          <%= title %>
        </span>
      </td>
    </tr>
    <tr style="">
      <td align="" style="padding: 4px;" colspan="4">
        <span class="chameleon-heading" style="padding-right: 4px; font-family:Helvetica, sans-serif; font-size:36px; line-height:32px; font-weight:100;color:#065d8c;"><%= value %></span>
        <span class="change">
          <%= (subValue1 ||= '').html_safe %>
        </span>
      </td>
    </tr>
    <tr style="">
      <td align="" style="" colspan="4">
        <div class="change-label ">
          <%= subValue2 ||= '' %>
          <%= subLabel2 ||= '' %>
        </div>
      </td>
    </tr>
  </tbody>
</table>
